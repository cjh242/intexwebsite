  <style>
    .custom-table-container {
        width: 100%;
        max-height: 500px; /* Set your desired height */
        overflow-y: auto;
      }
  
      .custom-table {
        width: 100%;
        border-collapse: collapse;
        margin-top: 20px; /* Adjust as needed */
      }
  
      .custom-header {
        background-color: #f2f2f2;
        z-index: 1;
      }
  
      .custom-header th,
      .custom-content td {
        border: 1px solid #ddd;
        padding: 8px;
        text-align: left;
      }
  
      /* Make the header fixed */
      .custom-header th {
        background-color: #f2f2f2;
        position: sticky;
        top: 0;
        z-index: 100;
      }
  
      /* Tooltip container */
      .tooltipConwayFav {
        position: relative;
        display: inline-block;
      }
  
      /* Tooltip text */
      .tooltipConwayFav .tooltiptextConwayFav {
        visibility: hidden;
        width: 160px;
        background-color: #333;
        color: #fff;
        text-align: center;
        border-radius: 6px;
        padding: 5px;
        position: absolute;
        z-index: 500;
        top: 200%; /* Adjusted to 100% */
        left: 50%;
        margin-left: -80px;
        opacity: 0;
        transition: opacity 0.3s;
      }
  
      /* Show the tooltip text when hovering */
      .tooltipConwayFav:hover .tooltiptextConwayFav {
        visibility: visible;
        opacity: 1;
      }
      .custom-table tr:nth-child(even) {
        background-color: #f9f9f9; /* Set your desired background color */
      }

  </style>

