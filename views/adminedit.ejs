<body class="page-contact">

 <!-- ======= Header ======= -->
 <header id="header" class="header d-flex align-items-center fixed-top">
    <div class="container-fluid container-xl d-flex align-items-center justify-content-between">

      <a href="/" class="logo d-flex align-items-center">
        <!-- Uncomment the line below if you also wish to use an image logo -->
        <!-- <img src="assets/img/logo.png" alt=""> -->
        <h1 class="d-flex align-items-center">Provo City</h1>
      </a>

      <i class="mobile-nav-toggle mobile-nav-show bi bi-list"></i>
      <i class="mobile-nav-toggle mobile-nav-hide d-none bi bi-x"></i>

      <nav id="navbar" class="navbar">
        <ul>
          <li><a href="/" class="active">Home</a></li>
          <li><a href="/survey">Survey</a></li>
          <li><a href="/dashboard">Explore Data</a></li>
          <% if (isAuthenticated) { %>
          <li><a href="/edit">Edit Profile</a></li>
          <li><a href="/register">Register</a></li>
          <li><a href="/results">Results</a></li>
          <% if (user.username === 'admin') { %>
            <li><a href="/adminedit">Manage Users</a></li>
            <% } %>
          <li><a href="/logout">Logout</a></li>
          <% } else { %>
          <li><a href="/login">Login</a></li>
          <% } %>
        </ul>
      </nav><!-- .navbar -->

    </div>
  </header><!-- End Header -->

  <main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <div class="breadcrumbs d-flex align-items-center" style="background-image: url('assets/img/mountains.jpg');">
      <div class="container position-relative d-flex flex-column align-items-center">

        <h2>Manage Users</h2>
        <ol>
          <li><a href="/">Home</a></li>
          <li>Manage Users</li>
        </ol>

      </div>
    </div><!-- End Breadcrumbs -->

    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
      <div class="container position-relative" data-aos="fade-up">

        <div class="row gy-4 d-flex justify-content-center">

          <div class="col-lg-6" data-aos="fade-up" data-aos-delay="250">

            <table>
                <thead>
                  <tr>
                      <th style="width: 20%;text-align:left">Username</th>
                      <th style="width: 17%;text-align:left">Email</th>
                      <th style="width: 10%;text-align:center">First Name</th>
                      <th style="width: 15%;text-align:left">Last Name</th>
                  </tr>
                </thead>
                <ul>
                  <% myusers.forEach(alluser => { %>
                    <% if(alluser.username != 'admin') { %>
                    <tr>
                    <td style="word-wrap: normal"><%= alluser.username %></td>
                    <td style="word-wrap: normal"><%= alluser.email %></td>
                    <td style="text-align:center"><%= alluser.firstName %></td>
                    <td style="word-wrap: normal"><%= alluser.lastName %></td>
                    <td style="width: 25%;text-align:right">
                    <form action="/deleteUser/<%= alluser.id %>" method="POST">
                      <input type="submit" name="Delete" value="Delete" />
                    </form>
                    </td>
                    
                  <% }}); %>
                  <% if (messages.error) { %>
                    <p style="color: red;"><%= messages.error %></p>
                <% } else if (messages.success) { %>
                    <p style="color: green;"><%= messages.success %></p>
                <% } %>
                </tr>
                </ul>
              <tbody>
  
              </tbody>
              </table>

          </div><!-- End Contact Form -->

        </div>

      </div>
    </section><!-- End Contact Section -->

  </main><!-- End #main -->

  <a href="#" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <div id="preloader"></div>

  <!-- Vendor JS Files -->
  <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="assets/vendor/aos/aos.js"></script>
  <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>

  <!-- Template Main JS File -->
  <script src="assets/js/main.js"></script>

</body>

</html>