    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
      <div class="container position-relative" data-aos="fade-up">

        <div class="row gy-4 d-flex justify-content-center">

          <div class="col-lg-20" data-aos="fade-up" data-aos-delay="250">

            <div class="custom-table-container">
            <table class="custom-table">
                <thead class="custom-header">
                  <tr>
                    <div>
                      <th style="width: 5%;text-align:left">Entry ID</th>
                      <th style="width: 5%;text-align:left">Age</th>
                      <th style="width: 10%;text-align:center">Gender</th>
                      <th style="width: 15%;text-align:left">Relationship Status</th>
                      <th style="width: 25%;text-align:left">Occupation Status</th>
                      <th style="width: 15%;text-align:left">Affiliated Platforms</th>
                      <th style="width: 15%;text-align:left">Affiliated Organizations</th>
                    </div>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q1
                        <span class="tooltiptextConwayFav">I often find myself using social media without a specific purpose</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q2
                        <span class="tooltiptextConwayFav">I often get distracted by social media when I am busy doing something</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q3
                        <span class="tooltiptextConwayFav">I am easily distracted
                        </span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q4
                        <span class="tooltiptextConwayFav">I am bothered by worries often</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q5
                        <span class="tooltiptextConwayFav">I find it difficult to concentrate on things</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q6
                        <span class="tooltiptextConwayFav">I often compare myself to other successful people through their use of social media</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q7
                        <span class="tooltiptextConwayFav">Following the previous statement, I end up feeling negatively about myself with these comparisons</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q8
                        <span class="tooltiptextConwayFav">I often seek validation through social media</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q9
                        <span class="tooltiptextConwayFav">I often feel depressed or down</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q10
                        <span class="tooltiptextConwayFav">My interest in daily activities fluctuates frequently</span>
                      </div></th>
                      <th style="width: 25%; text-align: left;">
                        <div class="tooltipConwayFav">
                        Q11
                        <span class="tooltiptextConwayFav">I often face issues regarding sleep</span>
                      </div></th>
                  </tr>
                </thead>
            </div>
                <ul>
                <% myrecords.forEach(record => { %>
                    <% let corg = ''; %>
                    <% let csocial = []; %>
                    <tr>
                        <td style="word-wrap: normal"><%= record.EntryID %></td>
                        <td style="word-wrap: normal"><%= record.Age %></td>
                        <td style="word-wrap: normal"><%= record.Gender %></td>
                        <td style="word-wrap: normal"><%= record.RelationshipStatus %></td>
                        <td style="word-wrap: normal"><%= record.OccupationStatus %></td>
                        <td><% mysocials.forEach(socialmedia => {
                            if(socialmedia.EntryID === record.EntryID) {
                                myplatforms.forEach(platform => {
                                    if(platform.PlatformID === socialmedia.PlatformID){ %>
                                        <% if(!csocial.includes(platform.Platform)) {
                                            csocial.push(platform.Platform); %>
                                                <%= platform.Platform %>
                                        <% } %>
                                <% }});}}); %> </td>
                            <td><% mysocials.forEach(socialmedia => {
                                if(socialmedia.EntryID === record.EntryID) {
                                myorganizations.forEach(organization => {
                                    if(organization.OrganizationID === socialmedia.OrganizationID){ %>
                                        <% if(corg !== organization.Organization) {
                                            corg = organization.Organization; %>
                                        <%= organization.Organization %>
                                <% }}}) %>
                            <% }}); %></td>
                        <td style="word-wrap: normal"><%= record.OftenSpent %></td>
                        <td style="word-wrap: normal"><%= record.Restless %></td>
                        <td style="word-wrap: normal"><%= record.Distracted %></td>
                        <td style="word-wrap: normal"><%= record.Worries %></td>
                        <td style="word-wrap: normal"><%= record.Concentration %></td>
                        <td style="word-wrap: normal"><%= record.CompareSuccess %></td>
                        <td style="word-wrap: normal"><%= record.FeelAboutComparison %></td>
                        <td style="word-wrap: normal"><%= record.SeekValidation %></td>
                        <td style="word-wrap: normal"><%= record.Depressed %></td>
                        <td style="word-wrap: normal"><%= record.InterestDailyActivites %></td>
                        <td style="word-wrap: normal"><%= record.Sleep %></td>
                    </tr>
                <% }); %>
                </tr>
                </ul>
              <tbody>
  
              </tbody>
              </table>

          </div><!-- End Contact Form -->

        </div>

